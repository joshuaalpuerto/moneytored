{
  "name": "Moneytored",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:docs": "docsify serve docs",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:clean": "rimraf ./coverage",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:eslint": "eslint",
    "lint:eslint:fix": "eslint --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:css": "stylelint 'App/**/*.js' --syntax scss",
    "lint:fix": "npm run lint:eslint:fix . && npm run lint:css",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "git-hook": "npm run lint -s && npm run test -s",
    "ac": "appcenter"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "apisauce": "^0.14.2",
    "appcenter": "1.13.0",
    "appcenter-analytics": "1.13.0",
    "appcenter-crashes": "1.13.0",
    "format-json": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.1",
    "querystringify": "1.0.0",
    "ramda": "0.26.1",
    "react": "16.8.3",
    "react-native": "0.59.5",
    "react-native-animatable": "1.3.2",
    "react-native-config": "^0.10.0",
    "react-native-device-info": "^0.24.3",
    "react-native-gesture-handler": "1.0.9",
    "react-native-i18n": "2.0.14",
    "react-native-vector-icons": "6.1.0",
    "react-navigation": "3.0.0",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "reduxsauce": "0.7.0",
    "seamless-immutable": "7.1.4",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@storybook/addon-storyshots": "^3.4.0",
    "@storybook/addons": "^3.4.0",
    "@storybook/channels": "^3.4.0",
    "@storybook/react-native": "^3.4.0",
    "appcenter-cli": "1.1.15",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^3.0.2",
    "docsify-cli": "4.3.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.17.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "ignite-andross": "3.0.4",
    "ignite-animatable": "1.0.2",
    "ignite-i18n": "1.2.0",
    "ignite-redux-persist": "1.1.2",
    "ignite-standard": "1.0.0",
    "ignite-vector-icons": "1.1.1",
    "jest": "24.7.1",
    "lint-staged": "8.1.5",
    "metro-react-native-babel-preset": "0.53.1",
    "metro-react-native-babel-transformer": "0.53.1",
    "mockery": "^2.1.0",
    "pre-commit": "1.2.2",
    "prettier": "1.17.0",
    "react-devtools-core": "3.4.3",
    "react-dom": "16.3.0",
    "react-test-renderer": "16.8.3",
    "reactotron-react-native": "^2.1.0",
    "reactotron-redux": "^2.1.0",
    "reactotron-redux-saga": "^2.1.0",
    "snazzy": "8.0.0",
    "stylelint": "15.10.1",
    "stylelint-config-react-native-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.6.0",
    "stylelint-react-native": "2.0.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/jest/"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js)$": "<rootDir>/jest/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/jest/Setup"
    ],
    "preset": "react-native"
  },
  "config": {}
}
